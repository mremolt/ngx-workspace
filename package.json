{
  "name": "ngx-workspace",
  "version": "1.0.0",
  "description": "NG multi app workspace example",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json -t verbose",
    "watch": "webpack-dev-server --config config/webpack.config.development.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.production.js",
    "watch:default-app": "cross-env APP_NAME=default-app npm run watch",
    "compile:default-app": "cross-env APP_NAME=default-app npm run build",
    "docs:default-app":
      "compodoc apps/default-app -n 'NGX Default App Docs' -t -p tsconfig.json -d reports/default-app/docs",
    "build:default-app": "run-s compile:default-app docs:default-app",
    "build:default-app:staging": "cross-env APP_ENV=staging run-s compile:default-app docs:default-app",
    "serve:default-app": "serve dist/default-app -p 8080 -c 0 -s"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^6.0.0-rc.6",
    "@angular/common": "^6.0.0-rc.6",
    "@angular/core": "^6.0.0-rc.6",
    "@angular/forms": "^6.0.0-rc.6",
    "@angular/platform-browser": "^6.0.0-rc.6",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.6",
    "@angular/router": "^6.0.0-rc.6",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.5",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0-alpha.2",
    "reselect": "^3.0.1",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.5.10",
    "@angular/compiler": "^6.0.0-rc.6",
    "@angular/compiler-cli": "^6.0.0-rc.6",
    "@angularclass/hmr": "^2.1.3",
    "@compodoc/compodoc": "^1.1.2",
    "@ngtools/webpack": "^6.0.0-rc.8",
    "@types/node": "^9.6.7",
    "@types/redux-logger": "^3.0.6",
    "@types/webpack-env": "^1.13.6",
    "app-root-path": "^2.0.1",
    "autoprefixer": "^8.4.1",
    "circular-dependency-plugin": "^5.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "is-primitive": "^3.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "offline-plugin": "^5.0.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^2.1.4",
    "raw-loader": "^0.5.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "rxjs-tslint-rules": "^4.0.2",
    "sass-loader": "^7.0.1",
    "serve": "^6.5.6",
    "ts-node": "^6.0.2",
    "tsconfig-paths": "^3.3.1",
    "tslint": "^5.9.1",
    "tslint-angular": "^1.1.1",
    "typescript": "2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "webpack-pwa-manifest": "^3.6.2",
    "webpack-subresource-integrity": "^1.1.0-rc.4"
  }
}
